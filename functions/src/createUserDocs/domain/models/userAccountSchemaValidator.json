{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "dateOfBirth": {
            "pattern": "^(-?(?:[1-9][0-9]*)?[0-9]{4})-(1[0-2]|0[1-9])-(3[01]|0[1-9]|[12][0-9])T(2[0-3]|[01][0-9]):([0-5][0-9]):([0-5][0-9])(.[0-9]+)?(Z)?$",
            "type": "string"
        },
        "dateOfInscription": {
            "pattern": "^(-?(?:[1-9][0-9]*)?[0-9]{4})-(1[0-2]|0[1-9])-(3[01]|0[1-9]|[12][0-9])T(2[0-3]|[01][0-9]):([0-5][0-9]):([0-5][0-9])(.[0-9]+)?(Z)?$",
            "type": "string"
        },
        "email": {
            "format": "email",
            "type": "string"
        },
        "location": {
            "type": "string"
        },
        "phoneNumber": {
            "type": "string"
        }
    },
    "required": [
        "dateOfInscription"
    ],
    "type": "object"
}

